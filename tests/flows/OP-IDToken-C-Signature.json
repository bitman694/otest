{
  "group": "ID Token",
  "sequence": [
    {
      "Webfinger": {
        "set_webfinger_resource": null
      }
    },
    {
      "Discovery": {
        "set_discovery_issuer": null
      }
    },
    "Registration",
    {
      "AsyncAuthn": {
        "set_response_where": null
      }
    },
    {
      "AccessToken": {
        "conditional_execution": {
          "return_type": ["CIT","CI","C","CT"]
        }
      }
    }
  ],
  "usage": {},
  "desc": "Does the OP sign the ID Token and with what",
  "assert": {
    "verify-response": {
      "response_cls": [
        "AccessTokenResponse",
        "AuthorizationResponse"
      ]
    },
    "is-idtoken-signed": {
      "alg": "RS256"
    }
  }
}