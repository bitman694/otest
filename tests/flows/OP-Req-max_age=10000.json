{
  "group": "Misc Request Parameters",
  "usage": {},
  "desc": "Requesting ID Token with max_age=10000 seconds restriction",
  "sequence": [
    {
      "Webfinger": {
        "set_webfinger_resource": null
      }
    },
    {
      "Discovery": {
        "set_discovery_issuer": null
      }
    },
    "Registration",
    {
      "AsyncAuthn": {
        "set_response_where": null
      }
    },
    "AccessToken",
    {
      "AsyncAuthn": {
        "set_response_where": null,
        "set_request_args": {
          "max_age": 10000
        }
      }
    },
    "AccessToken"
  ],
  "MTI": [
    "C",
    "I",
    "IT",
    "CI",
    "CT",
    "CIT"
  ],
  "assert": {
    "claims-check": {
      "required": true,
      "id_token": [
        "auth_time"
      ]
    },
    "same-authn": null,
    "auth_time-check": {
      "max_age": 10000,
      "skew": 600
    },
    "verify-response": {
      "response_cls": [
        "AccessTokenResponse",
        "AuthorizationResponse"
      ]
    }
  }
}